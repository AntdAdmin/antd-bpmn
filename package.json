  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "antd-bpmn",
    "antdesign bpmn",
    "react",
    "antd",
    "ant.design"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
